{
  "name": "@monoai/error-tracker-sdk",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w @monoai/error-tracker-core",
    "build:browser": "npm run build -w @monoai/error-tracker-browser",
    "build:node": "npm run build -w @monoai/error-tracker-node",
    "clean": "npm run clean --workspaces --if-present",
    "typecheck": "tsc --build",
    "lint": "eslint packages/*/src --ext .ts,.tsx"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.0",
    "rollup": "^4.9.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/monoAItechnology/custom-error-tracker-sdk-js.git"
  },
  "license": "MIT"
}

    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c && tsc --emitDeclarationOnly --outDir dist/types",
    "clean": "rm -rf dist"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/monoAItechnology/custom-error-tracker-sdk-js.git",
    "directory": "packages/core"
  },
